{
    "name": "onilopic/api-tdd",
    "description": "Test Driven PHP",
    "type": "project",
    "require-dev": {
        "pestphp/pest": "^2.34",
        "symfony/var-dumper": "^7.0"
    },
    "version": "8.2",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests"
      }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
